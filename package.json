{
  "name": "bullfrog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "heroku-postbuild": "cd client && yarn && yarn run build",
    "client": "cd client && yarn start",
    "dev-server": "nodemon ./bin/www set PORT=5000",
    "dev-start": "concurrently --kill-others-on-fail \"yarn dev-server\" \"yarn client\""
  },
  "dependencies": {
    "cloudinary": "^1.14.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "~4.17.3",
    "knackhq-client": "https://github.com/john-raymon/knackhq-client.git",
    "mongoose": "^5.4.17",
    "mongoose-find-or-create": "^1.3.1",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "multer-storage-cloudinary": "^2.2.1",
    "node-sass-middleware": "0.11.0",
    "qr-image": "^3.2.0",
    "superagent": "^4.1.0",
    "superagent-promise": "^1.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
